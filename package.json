    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "socket.io-client": "^4.5.3",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.3.0",
    "ws": "^8.4.0"
